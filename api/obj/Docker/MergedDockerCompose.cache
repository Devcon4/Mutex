networks:
  mutex_default: {}
services:
  db:
    environment:
      POSTGRES_DB: mutex
      POSTGRES_PASSWORD: MutexDev
      POSTGRES_USER: dev
    image: postgres:11.5
    networks:
      mutex_default: {}
    ports:
    - published: 5432
      target: 5432
    volumes:
    - dbdata:/var/lib/postgresql/data:rw
  mutex-api:
    build:
      context: C:\Code\Mutex\api
      dockerfile: mutex-api/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_HTTPS_PORT: '44366'
      ASPNETCORE_URLS: https://+:443;http://+:80
    image: mutexapi
    networks:
      mutex_default: {}
    ports:
    - published: 10001
      target: 80
    - published: 44366
      target: 443
    volumes:
    - C:\Users\devco\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\devco\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
version: '3.4'
volumes:
  dbdata: {}