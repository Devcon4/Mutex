networks:
  mutex_default: {}
services:
  db:
    environment:
      POSTGRES_DB: mutex
      POSTGRES_PASSWORD: MutexDev
      POSTGRES_USER: dev
    image: postgres:11.5
    networks:
      mutex_default: {}
    ports:
    - published: 5432
      target: 5432
    volumes:
    - dbdata:/var/lib/postgresql/data:rw
  mutex-api:
    build:
      context: C:\Code\Mutex\api
      dockerfile: mutex-api/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: mutex-api
      target: base
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_HTTPS_PORT: '44366'
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
    image: mutexapi:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  "bin/Debug/netcoreapp3.0/mutex-api.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    networks:
      mutex_default: {}
    ports:
    - published: 10001
      target: 80
    - published: 44366
      target: 443
    tty: true
    volumes:
    - /c/Code/Mutex/api/mutex-api:/app:rw
    - /c/Users/devco/vsdbg/vs2017u5:/remote_debugger:rw
    - /c/Users/devco/AppData/Roaming/ASP.NET/Https:/root/.aspnet/https:ro
    - /c/Users/devco/AppData/Roaming/Microsoft/UserSecrets:/root/.microsoft/usersecrets:ro
    - /c/Program Files/dotnet/sdk/NuGetFallbackFolder:/root/.nuget/fallbackpackages:ro
    - /c/Users/devco/.nuget/packages:/root/.nuget/packages:ro
    - /c/Code/Mutex/api:/src:rw
version: '3.4'
volumes:
  dbdata: {}